{
  "name": "react-rinter",
  "version": "0.2.0",
  "description": "React components for Rinter",
  "main": "lib/index.js",
  "module": "lib-esm/index.js",
  "repository": "https://github.com/dfernandez79/react-rinter.git",
  "author": "Diego Fernandez <diegof79@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "keywords": [
    "state",
    "predictable",
    "reducer",
    "rxjs",
    "flux",
    "elm",
    "react",
    "rinter"
  ],
  "peerDependencies": {
    "react": "^16.3.0",
    "rxjs": "6.x"
  },
  "dependencies": {
    "create-subscription": "^16.3.0",
    "prop-types": "^15.5.8",
    "rinter": "^2.0.0-12"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "ava": "^1.4.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.12.4",
    "jsdom": "^14.0.0",
    "lodash": "^4.17.11",
    "np": "^4.0.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.3.0",
    "parcel-bundler": "^1.12.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rimraf": "^2.6.3",
    "rxjs": "^6.4.0"
  },
  "scripts": {
    "build": "npm-run-all build:lib build:esm",
    "build:lib": "babel src -d lib",
    "build:esm": "babel src -d lib-esm --env-name esm",
    "test": "ava",
    "coverage": "npm-run-all build:lib coverage:run",
    "coverage:run": "nyc --reporter=html ava",
    "clean": "rimraf lib lib-esm *.tgz coverage .nyc_output *.log .cache dist",
    "lint": "eslint src",
    "release": "npm-run-all clean lint build release:np",
    "release:np": "np",
    "serve:counter": "parcel serve examples/counter/index.html",
    "serve:todo": "parcel serve examples/todo/index.html",
    "serve:numbers": "parcel serve examples/numbers/index.html"
  },
  "ava": {
    "files": [
      "src/**/*.test.js"
    ],
    "require": [
      "@babel/register"
    ],
    "babel": {
      "testOptions": {
        "presets": [
          "@babel/preset-react"
        ],
        "babelrc": false
      }
    }
  }
}
