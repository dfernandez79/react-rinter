{
  "name": "react-rinter",
  "version": "0.1.0",
  "description": "React components for Rinter",
  "main": "lib/index.js",
  "module": "lib-esm/index.js",
  "repository": "https://github.com/dfernandez79/react-rinter.git",
  "author": "Diego Fernandez <diegof79@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "keywords": [
    "state",
    "predictable",
    "reducer",
    "rxjs",
    "flux",
    "elm",
    "react",
    "rinter"
  ],
  "peerDependencies": {
    "rxjs": "6.x",
    "react": "^16.3.0"
  },
  "dependencies": {
    "create-subscription": "^16.3.0",
    "prop-types": "^15.5.8",
    "rinter": "^2.0.0-11"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "cpy-cli": "^2.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.12.4",
    "lodash": "^4.17.11",
    "np": "^4.0.2",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rimraf": "^2.6.3",
    "rxjs": "^6.4.0"
  },
  "scripts": {
    "build": "npm-run-all build:lib build:esm",
    "build:lib": "babel src -d lib",
    "build:esm": "cpy '**/*.js' '!**/*.test.js' --cwd=src ../lib-esm",
    "test": "ava",
    "coverage": "npm-run-all build:lib coverage:run",
    "coverage:run": "nyc --reporter=html ava",
    "clean": "rimraf lib lib-esm *.tgz coverage .nyc_output *.log .cache dist",
    "lint": "eslint src",
    "release": "npm-run-all clean lint build release:np",
    "release:np": "np",
    "serve:counter": "parcel serve examples/counter/index.html",
    "serve:todo": "parcel serve examples/todo/index.html"
  }
}
